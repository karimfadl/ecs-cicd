{
  "family": "ecs-app",
  "containerDefinitions": [
    {
      "name": "ecs-app-container",
      "image": "611135556662.dkr.ecr.us-east-2.amazonaws.com/ecs-repo:v_%BUILD_NUMBER%",
      "memory": 256,
      "cpu": 10,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
